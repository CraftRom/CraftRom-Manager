<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.CraftRomManager" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/blue_300</item>
        <item name="colorPrimaryVariant">@color/blue_500</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/blue_300</item>
        <item name="colorSecondaryVariant">?attr/colorPrimary</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="colorSurface">#0D0D0D</item>
        <item name="colorSurfaceVariant">#333335</item>
        <item name="colorSurfaceSurfaceVariant">#292929</item>
        <item name="colorOnBackground">?colorOnSurface</item>
        <item name="colorError">#EF8282</item>
        <item name="colorOnError">#0D0D0D</item>
        <item name="colorOnSurface">#D8D8D8</item>
        <item name="colorOnSurfaceVariant">#CCBABABA</item>
    </style>

</resources>